!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------

Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight
URxvt*termName: rxvt-unicode-256color
URxvt*saveLines: 4096
URxvt*loginShell: True
URxvt*hold: false
URxvt.scrollstyle:      plain
URxvt.secondaryScroll:  true


URxvt.font: xft:Iosevkasize=13
URxvt*letterSpace     : -1
! Allow switching of font sizes
/* Set a icon file for URxvt */
! URxvt*iconFile: ~/terminal.svg
URxvt*gemetry: 120x44
URxvt*reverseVideo: False
URxvt*depth: 32
URxvt*background: #292f33
URxvt*fadeing: 30
URxvt*visualBell: True
URxvt*cursorBlink: True
URxvt*scrollstyle: plain
URxvt*scrollColor: #777777
URxvt*thickness: 5
URxvt*scrollWithBuffer: True
URxvt*skipScroll: True
URxvt*borderColor: #000000
URxvt*borderLess: False
URxvt*underlineURLs: True
URxvt*iso14755: False
URxvt*iso14755_52: False


URxvt.perl-ext: default,url-select,clipboard,resize-font
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0



