
/*
 * X resource file for URxvt
 * vim:ft=xdefaults:
 *
 * Maintained by Claud D. Park <posquit0.bj@gmail.com>
 * http://www.posquit0.com/
 */


URxvt*termName: rxvt-unicode-256color
URxvt*saveLines: 4096
URxvt*loginShell: True
URxvt*hold: false


URxvt*font: \
    xft:Iosevka:size=13:antialias=true:hinting=true, \
    xft:NanumGothicCoding:size=12:antialias=true:hinting=true
URxvt*italicFont: \
    xft:Iosevka:size=12:italic:antialias=true:hinting=true, \
    xft:NanumGothicCoding:size=12:italic:antialias=true:hinting=true
URxvt*boldFont: \
    xft:Iosevka:size=12:bold:antialias=true:hinting=true, \
    xft:NanumGothicCoding:size=12:bold:antialias=true:hinting=true
URxvt*boldItalicFont: \
    xft:Iosevka:size=12:bold:italic:antialias=true:hinting=true, \
    xft:NanumGothicCoding:size=12:bold:italic:antialias=true:hinting=true
URxvt*buffered: True
URxvt*letterSpace: 0
URxvt*lineSpace: 0
URxvt*inputMethod: ibus
URxvt*preeditType: OffTheSpot
URxvt*imLocale: en_US.UTF-8
URxvt*imFont: NanumGothic


/* Set a icon file for URxvt */
! URxvt*iconFile: ~/terminal.svg
URxvt*gemetry: 120x44
URxvt*reverseVideo: False
URxvt*depth: 32
URxvt*background: [90]#111111
URxvt*fadeing: 30
URxvt*visualBell: True
URxvt*cursorBlink: True
URxvt*scrollBar: True
URxvt*scrollBar_right: True
URxvt*scrollstyle: plain
URxvt*scrollColor: #777777
URxvt*thickness: 5
URxvt*scrollTtyKeypress: True
URxvt*scrollTtyOutput: False
URxvt*scrollWithBuffer: True
URxvt*skipScroll: True
URxvt*borderColor: #000000
URxvt*borderLess: False
URxvt.internalBorder: 0
URxvt.externalBorder: 0
URxvt*underlineURLs: True
URxvt*iso14755: False
URxvt*iso14755_52: False


URxvt.perl-ext: default,url-select,clipboard,tabbed,resize-font
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0



!! Colorscheme

! special
*.foreground: #93a1a1
*.background: #141c21
*.cursorColor: #afbfbf

! black
*.color0: #263640
*.color8: #4a697d

! red
*.color1: #d12f2c
*.color9: #fa3935

! green
*.color2: #819400
*.color10: #a4bd00

! yellow
*.color3: #b08500
*.color11: #d9a400

! blue
*.color4: #2587cc
*.color12: #2ca2f5

! magenta
*.color5: #696ebf
*.color13: #8086e8

! cyan
*.color6: #289c93
*.color14: #33c5ba

! white
*.color7: #bfbaac
*.color15: #fdf6e3
!! URxvt Appearance
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false
